{
  "expo": {
    "name": "Blade Buddy",
    "slug": "bladebuddy",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/LogoDark.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.westmorelandcreative.bladebuddy"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.westmorelandcreative.bladebuddy"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "2a77fa98-4df6-4de3-86f5-6e3e50e2c619"
      }
    },
    "owner": "rawestmoreland",
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/2a77fa98-4df6-4de3-86f5-6e3e50e2c619"
    }
  }
}
