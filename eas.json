{
  "cli": {
    "version": ">= 3.5.2",
    "appVersionSource": "remote",
    "promptToConfigurePushNotifications": false
  },
  "build": {
    "development": {
      "channel": "development",
      "distribution": "internal",
      "developmentClient": true,
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "app-bundle",
        "image": "latest"
      },
      "env": {
        "APP_ENV": "development"
      }
    },
    "development-simulator": {
      "extends": "development",
      "ios": {
        "simulator": true
      },
      "env": {
        "APP_ENV": "development"
      }
    },
    "staging": {
      "channel": "staging",
      "autoIncrement": true,
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "app-bundle",
        "image": "latest"
      },
      "env": {
        "APP_ENV": "staging"
      }
    },
    "production": {
      "channel": "production",
      "autoIncrement": true,
      "distribution": "store",
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "buildType": "app-bundle",
        "image": "latest"
      },
      "env": {
        "APP_ENV": "production"
      }
    }
  },
  "submit": {
    "staging": {
      "ios": {
        "appleId": "support@westmorelandcreative.com",
        "appleTeamId": "KUF6U27VW2",
        "ascAppId": "6475628254"
      }
    },
    "production": {
      "ios": {
        "appleId": "support@westmorelandcreative.com",
        "appleTeamId": "KUF6U27VW2",
        "ascAppId": "6475628254"
      }
    }
  }
}
